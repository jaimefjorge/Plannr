@model IEnumerable<Plannr.Models.DemandeReservation>

@{
    ViewBag.Title = "Index";
}

<h2>Mes demandes de réservation</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateDemande)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Checked)
        </th>
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateDemande)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Creneau.HeureDebut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Checked)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enseignement.Enseignant.UserName)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.Enseignement.Cours.Matiere.Ue.Libelle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enseignement.Cours.Libelle)
            </td>
            <td>
      
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
